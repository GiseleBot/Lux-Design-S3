{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Debug Phteven vs default kits",
            "type": "debugpy",
            "request": "launch",
            "program": "./src/luxai_runner/cli.py",
            "console": "integratedTerminal",
            "args": [
                "./python/main.py",
                "./kits/python/main.py",
                "--output",
                "./replays/replay.html"
            ]
        },
        {
            "name": "Python Debugger: Debug Gisele vs default kits",
            "type": "debugpy",
            "request": "launch",
            "program": "./src/luxai_runner/cli.py",
            "console": "integratedTerminal",
            "args": [
                "./Gisele/main.py",
                "./kits/python/main.py",
                "--output",
                "./replays/replay.html"
            ]
        },
    ]
}